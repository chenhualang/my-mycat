<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.integration.dao.CourierDAO">
	<resultMap id="result" type="com.example.common.dto.CourierDTO">
		<result column="id" jdbcType="VARCHAR" property="id" />
		<result column="name" jdbcType="VARCHAR" property="name" />
	</resultMap>

	<insert id="add" parameterType="com.example.common.dto.CourierDTO">
		INSERT INTO
		courier(id, name)
		VALUES(#{id}, #{name})
	</insert>

	<select id="getCourierList" parameterType="java.lang.String" resultMap="result">
		SELECT * FROM courier where nameÔºù#{name,jdbcType=VARCHAR}
	</select>
</mapper>